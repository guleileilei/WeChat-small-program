page{
    height: 100%;
    background-color: #fff;
}
.container{
    display: flex;
    flex-direction: column;
    align-items: center;
    background-color:#fff; 
    padding: 0; 
}
.user-name{
    font-size: 51rpx;
    font-weight: bold;
    line-height: 73rpx;
    margin-top:60rpx;
    color: deeppink;
}
.user-myke{
    font-size: 40rpx;
    color:deeppink;
    line-height: 64rpx;
    margin-bottom:60rpx;
}
.container input{
    float: right;
}
.input_sj{
    width:528rpx;
}
.ip_name{
    flex: 1;
    flex-direction: row;
    height:100rpx;
    font-size:30rpx;
    line-height:100rpx;
    border-bottom: 1rpx #f5f5f5 solid;
    color:#4d9df7;
    position: relative;
}
.ip_name image{
    width:40rpx;
    height:30rpx;
    margin-right:20rpx;
}
.telIn{
    display: flex;
}
.loginInput{
    line-height:100rpx;
    height: 100rpx;
    margin:auto 0;
}
.telIn .loginInput{
    width: 220rpx;
}
.telIn image{
    margin:auto 0;
    margin-right:20rpx;
}
.button_fs{
    position: absolute;
    font-size: 17rpx;
    right: 0;
    width: 148rpx;
    height: 45rpx;
    float: right;
    border-radius: 25rpx;
    border: 1px #4d9df7 solid;
    text-align: center;
    line-height: 45rpx;
    top: 29rpx;
    color: #4d9df7;
}
.dl .bjt{
    width: 418rpx;
    height: 180rpx;
    margin-left: 60rpx;
    display: block;
    margin-top: 20rpx;
}
.dl .denglu,.addTags{
    width:555rpx;
    height: 70rpx;
    background: #4d9df7;
    color: #fff;
    border-radius: 50rpx;
    margin-bottom:20rpx; 
    line-height: 70rpx;
}
.addTags{
    background-color: deeppink;
    color: #fff;
    margin-top:20rpx;
}
.dl .kankan,.dl .weixin{
    width:555rpx;
    height: 70rpx;
    line-height: 70rpx;
    color: #333;
    border-radius: 50rpx;
    border: 1rpx #eee solid;
    margin-bottom:20rpx; 
}
.dl .weixin{
    background: #4d9df7;
    color: #fff;
}
