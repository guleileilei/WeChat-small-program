/**index.wxss**/
.warp{
  background-color: #f5f8fa;
  overflow: hidden;
}
.header{
  background-color: #fff;
  padding-top:16rpx;
  overflow: hidden;
}
.header_l{
  padding-left: 24rpx;
  float: left;
}
.header_l image{
  width:40rpx;
  height:40rpx;
}
.header_l text{
  font-weight: bold;
  line-height: 40rpx;
  color: #666666;
}
.header_r{
  float: right;
  padding-right: 20rpx;
}
.header_r image{
  width:40rpx;
  height:40rpx;
}
.header_r text{
  font-weight: bold;
  line-height: 40rpx;
  padding-left: 22rpx;
  color: #666666;
}
.list{
  margin-top:12rpx;
  background-color: #fff;
  padding: 30rpx 33rpx 32rpx 28rpx;
}
.school{
  display: inline-block;
  margin-right: 50rpx;
}
.school text{
  font-size: 24rpx;
  font-weight: bold;
  color:#4d9df7;
  padding-left: 10rpx;
}
.time{
  display: inline-block;
}
.time text{
  font-size: 24rpx;
  color:#4d9df7;
  padding-left: 13rpx;
}
.list image{
  height:34rpx;
  width: 34rpx;
}
.remand{
  display: inline-block;
  border: 1px solid #71b1f9;
  border-bottom-left-radius: 8rpx;
  border-bottom-right-radius: 8rpx; 
  margin-left: 102rpx;
  color: #71b1f9;
  font-size: 24rpx;
  padding: 10rpx 23rpx;
  width: 96rpx;
  text-align: center;
}
.title{
  font-weight: bold;
  margin-bottom: 24rpx;
}
.chapter{
  font-size: 16px;
  color:#999999;
}
.foot{
  height: 50rpx;
  margin-top: 12rpx;
  background-color: #fff;
  text-align: center;
  padding-top: 260rpx;
}
.foot span{
  border-bottom: 1px solid #999999;
  font-size: 16px;
  color: #999999;
  padding-bottom: 5rpx;
}
